{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "en-yup-decorator",
  "author": "Mattia Pispisa",
  "module": "dist/en-yup-decorator.esm.js",
  "size-limit": [
    {
      "path": "dist/en-yup-decorator.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/en-yup-decorator.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "reflect-metadata": "^0.1.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "husky": "^9.1.7",
    "size-limit": "^11.1.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "yup": "^1.0.0"
  },
  "peerDependencies": {
    "yup": "^1.0.0"
  }
}
